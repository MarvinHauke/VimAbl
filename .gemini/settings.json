{
  "mcpServers": {
    "svelte": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@sveltejs/mcp"],
      "env": {}
    },
    "AbletonMCP": {
      "command": "/Users/pforsten/Development/python/ableton-mcp-extended/.venv/bin/python3",
      "args": [
        "/Users/pforsten/Development/python/ableton-mcp-extended/MCP_Server/server.py"
      ]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": ["-y", "chrome-devtools-mcp@latest"]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "context7": {
      "httpUrl": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "CONTEXT7_API_KEY",
        "Accept": "application/json, text/event-stream"
      }
    },
    "serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--project",
        "/Users/pforsten/Development/python/VimAbl/"
      ],
      "env": {}
    }
  }
}
